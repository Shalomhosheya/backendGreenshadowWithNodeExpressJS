generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Crop {
  cropID         String @id @default(uuid())
  cropName       String
  cropImage      Bytes
  scientificName String
  category       String
  season         String
}

model Vehicle {
  vehicleID       String @id @default(uuid())
  vehicleCategory String
  fueltype        String
  remarks         String
  status          String
  licenseplate    String
}

model equipment {
  equipID String @id @default(uuid())
  name    String
  type    String
  status  String
}

model Field {
  fieldID       String @id
  fieldName     String
  fieldLocation String
  fieldSize     String
  fieldStaff    String
  pic1          Bytes?
  pic2          Bytes?
}
